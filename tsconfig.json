{
    "display": "Node 20",
    "_version": "20.1.0",
    "compilerOptions": {
        "lib": [
            "es2023"
        ],
        "module": "node16",
        "target": "es2022",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "moduleResolution": "node16",
        "rootDir": "src",
        "outDir": "out"
    },
    "exclude": [
        "out",
        "node_modules"
    ]
}